{
  "words": [],
  "allowCompoundWords": false,
  "dictionaries": [
    "en_US",
    "node",
    "npm",
    "typescript",
    "filetypes",
    "my_word",
    "programming_word",
    "knowre_bridge"
  ],
  "language": "en",
  "dictionaryDefinitions": [
    {
      "path": "~/.config/nvim/cspell/my-dictionary.txt",
      "name": "my_word",
      "addWords": true
    },
    {
      "path": "~/.config/nvim/cspell/programming.txt",
      "name": "programming_word",
      "addWords": true
    },
    {
      "path": "~/.config/nvim/cspell/knowre-bridge.txt",
      "name": "knowre_bridge",
      "addWords": true
    }
  ],
  "ignorePaths": ["node_modules", ".git", ".vscode"],
  "version": "0.2",
  "ignoreRegExpList": ["\\b\\d+[a-z]{1,2}\\b"],
  "flagWords": ["histroy", "calender"]
}
